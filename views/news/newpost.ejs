  <div>
    <!--Open principal card-->
    <div class="card" style="width: 100%;">
      <div class="card-body">
        <!-- Open navbar-->
        <ul class="nav justify-content-end">
          <li class="nav-item">
            <a type="button" class="btn btn-outline-success" class="nav-link active" href="/new">Add new Post</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Link</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Link</a>
          </li>
          <li class="nav-item">
             <img src="/docs/4.0/assets/brand/bootstrap-solid.svg" width="30" height="30" alt="">
          </li>
        </ul>
        <!--End navbar-->
        <!--Open tittle-->
        <h1 class="card-title">
          <!-- ejsout -->
          <%= titulo %>
        </h1>
        <!--End tittle-->

        <!--Open lateral menu-->
        <div class="row">
          <div class="col-4">
            <!-- Colum -->
            <div class="list-group" id="list-tab" role="tablist">
              <a class="list-group-item list-group-item-action active" id="list-home-list" data-toggle="list"
                href="#list-home" role="tab" aria-controls="home">Home</a>
              <a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list"
                href="#list-profile" role="tab" aria-controls="profile">Profile</a>
              <a class="list-group-item list-group-item-action" id="list-messages-list" data-toggle="list"
                href="#list-messages" role="tab" aria-controls="messages">Messages</a>
              <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list"
                href="#list-settings" role="tab" aria-controls="settings">Settings</a>
            </div>
          </div>
          <!--End colum-->

          <div class="col-8">
            <!-- Post box-->
            <div class="tab-content" id="nav-tabContent" class="ex3">
              <div class="tab-pane fade show active" id="list-home" role="tabpanel" aria-labelledby="list-home-list"
                data-spy="scroll" data-target="#list-example" data-offset="0" class="scrollspy-example">
                <div href="list-home">
                  <!-- ejseach -->
                  <% travels.forEach(travel => { %>
                  <li>
                    <h3>
                      <%= travel.postname %>
                    </h3>
                    <br>
                    <p>
                      <%= travel.description %>
                    </p>
                    <!-- Fecha de creacion de registro en la base de datos -->
                    <p>
                      <%= travel.createdAt %>
                    </p>
                    <a class="btn btn-primary" href="/detail/ <%= travel.id %> " role="button">Look more</a>
                    <a class="btn btn-primary" href="/edit/<%= travel.id %> " role="button">Edit post</a>
                    <a class="btn btn-primary" href="/delete/<%= travel.id %> " role="button">Delete post</a>
                  </li>
                  <hr>
                  <br>
                  <% }) %>
                </div>
              </div>
              <div class="tab-pane fade" id="list-profile" role="tabpanel" aria-labelledby="list-profile-list">...</div>
              <div class="tab-pane fade" id="list-messages" role="tabpanel" aria-labelledby="list-messages-list">...
              </div>
              <div class="tab-pane fade" id="list-settings" role="tabpanel" aria-labelledby="list-settings-list">...
              </div>
            </div>
          </div>
          <!--End post-->
        </div>
        <!--End lateral menu-->

      </div>
    </div>

  </div>



